# This is ~/.ssh/config
# # For all hosts
ServerAliveInterval 10

Host *
  IdentityFile ~/.ssh/richard.pem

Host digitalstar
  HostName ssh.digitalstar.tech
  User root
  IdentityFile ~/.ssh/richard.pem

Host digitalstar.git
  HostName ssh.digitalstar.tech
  User dokku
  IdentityFile ~/.ssh/richard.pem
